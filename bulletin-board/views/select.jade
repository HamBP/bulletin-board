extends layout

block profile
  form.login_form(action="", method="post")
    div
      label ID
      br
      input#id(type="text", name="id")
    div
      label Password
      br
      input#password(type="password", name="password")
    button(type="submit") sign in
    button(type="submit") sign up

block content
    h2= article.title
    p= article.content